{
  "name": "rbxmx-to-ts",
  "version": "1.0.6",
  "description": "A powerful CLI to create .d.ts files for .rbxmx files",
  "main": "out/index.js",
  "bin": {
    "rbxmxc": "out/index.js"
  },
  "engines": {
    "node": ">=12 <13 || >=14 <15 || >=16"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepackage": "tsc",
    "package": "pkg -o rbxmxc -t node16-win-x64,node16-macos-x64,node16-linux-x64 out/index.js",
    "prepublishOnly": "tsc"
  },
  "author": "sasial-dev",
  "repository": "https://github.com/sasial-dev/rbxmx-to-ts.git",
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "commander": "^8.0.0",
    "glob": "^7.1.7",
    "strip-json-comments": "4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@tsconfig/node12": "1.0.9",
    "@types/glob": "7.1.4",
    "@types/xml2js": "0.4.9",
    "pkg": "5.3.2",
    "typescript": "4.3.5"
  }
}
